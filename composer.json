{
    "name": "maicol07/blade-mdi",
    "description": "Material Design Icons for Laravel Blade views.",
    "keywords": ["laravel", "php", "blade", "ui", "kit", "icons", "mdi", "material", "design", "icons"],
    "license": "MIT",
    "homepage": "https://github.com/maicol07/blade-mdi",
    "authors": [
        {
            "name": "inerba",
            "homepage": "https://github.com/inerba",
            "role": "Developer"
        },
        {
            "name": "maicol07",
            "homepage": "https://github.com/maicol07",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-icons": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Maicol07\\BladeMdi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maicol07\\BladeMdi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Maicol07\\BladeMdi\\BladeMdiServiceProvider"
            ]
        }
    }
}
